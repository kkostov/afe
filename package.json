{
    "name": "afe",
    "version": "0.1.0",
    "private": true,
    "main": "app/index.js",
    "dependencies": {
        "react": "^15.4.2",
        "react-dom": "^15.4.2"
    },
    "devDependencies": {
        "babel-eslint": "7.0.0",
        "electron": "^1.6.1",
        "eslint": "3.16.1",
        "eslint-config-airbnb-base": "^11.1.0",
        "eslint-config-react-app": "0.3.0",
        "eslint-plugin-flowtype": "2.21.0",
        "eslint-plugin-import": "2.2.0",
        "eslint-plugin-jsx-a11y": "2.2.3",
        "eslint-plugin-react": "6.4.1",
        "foreman": "^2.0.0",
        "react-scripts": "0.9.3"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "airbnb-base"
        ],
        "plugins": [
            "react"
        ],
        "env": {
            "browser": true
        },
        "parserOptions": {
            "ecmaFeatures": {
                "jsx": true
            }
        },
        "rules": {
            "semi": [
                "error",
                "never"
            ],
            "indent": [
                "error",
                4
            ],
            "react/jsx-uses-react": "error",
            "class-methods-use-this": "off",
            "import/no-extraneous-dependencies": [
                "off"
            ],
            "global-require": [
                "off"
            ]
        }
    },
    "homepage": ".",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "electron": "electron .",
        "dev": "nf start"
    }
}
